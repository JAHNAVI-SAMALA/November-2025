class Solution:
    def maxSumDivThree(self, nums: List[int]) -> int:
        dp = [0,float('-inf'),float('-inf')]
        for num in nums:
            old = dp[:]
            for r in range(3):
                new_rem = (r+num)%3
                dp[new_rem] = max(dp[new_rem],old[r] + num)
        return dp[0]
